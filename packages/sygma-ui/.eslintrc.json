{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": ["plugin:@typescript-eslint/recommended"],
  "plugins": ["only-warn"],
  "rules": {
    "no-unused-vars": "off"
  }
}
